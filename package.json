{
  "name": "ReactAppBoilerplate",
  "version": "1.0.0",
  "description": "This is a basic boiler plate with routing and redux preconfigured.",
  "main": "index.js",
  "scripts": {
    "analyze": "npm run build && source-map-explorer 'build/*.js'",
    "prebuild": "npm run build:clean",
    "build": "npm run build:clean && cross-env NODE_ENV=production BROWSERSLIST_ENV=production webpack --config config/webpack.production.config.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development BROWSERSLIST_ENV=development node server",
    "start:prod": "cross-env NODE_ENV=production BROWSERSLIST_ENV=production node server",
    "start:production": "npm run build && npm run start:prod",
    "lint": "eslint src/ --fix",
    "pretty": "pretty-quick --staged",
    "test": "jest --config config/jest/jest.config.js",
    "test:watch": "npm run test -- --watchAll",
    "browser-list": "cross-env BROWSERSLIST_ENV=development npx browserslist",
    "browser-list:prod": "cross-env BROWSERSLIST_ENV=production npx browserslist"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "localforage": "^1.9.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-persist": "6.0.0",
    "redux-persist-transform-compress": "4.2.0",
    "redux-persist-transform-encrypt": "^3.0.1",
    "redux-thunk": "2.3.0",
    "webpack-version-file": "^0.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "babel-minify-webpack-plugin": "^0.3.1",
